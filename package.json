{
  "name": "udes-auth-plugins",
  "version": "0.0.1",
  "description": "Collection of plugins to ease various authentication methods",
  "main": "./dist/index.js",
  "scripts": {
    "audit": "node_modules/.bin/nsp check",
    "build": "babel ./src --out-dir dist --ignore '**/*Test.js'",
    "coverage": "NODE_ENV=test node_modules/.bin/nyc --require babel-core/register -x '**/**Test.js' node_modules/.bin/mocha --opts ../mocha.opts \\ **/**Test.js",
    "lint": "eslint ./src",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --opts ../mocha.opts **/**Test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UdeS-STI/udes-auth-plugins.git"
  },
  "author": "Jessy Lachapelle <jessy.lachapelle@usherbrooke.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UdeS-STI/udes-auth-plugins/issues"
  },
  "dependencies": {
    "request": "2.83.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.1.2",
    "eslint-config-udes": "0.1.6",
    "mocha": "4.0.1",
    "nsp": "2.8.1",
    "nyc": "11.3.0"
  },
  "homepage": "https://github.com/UdeS-STI/udes-auth-plugins#readme"
}
