{
  "name": "udes-auth-plugins",
  "version": "0.1.0",
  "description": "Collection of plugins to ease various authentication methods",
  "main": "./src/index.js",
  "scripts": {
    "audit": "node_modules/.bin/nsp check",
    "build": "babel ./src --out-dir dist --ignore '**/*Test.js'",
    "coverage": "NODE_ENV=test node_modules/.bin/nyc --require babel-core/register -x '**/**Test.js' node_modules/.bin/mocha --opts ../mocha.opts \\ ./src**/**Test.js",
    "format": "eslint . --ext js,json --ignore-path .gitignore --fix",
    "lint": "eslint . --ext js,json --ignore-path .gitignore",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --opts ../mocha.opts ./src/**/**Test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UdeS-STI/udes-auth-plugins.git"
  },
  "author": "Universit√© de Sherbrooke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UdeS-STI/udes-auth-plugins/issues"
  },
  "dependencies": {
    "request": "^2.83.0",
    "udes-node-orchestrator": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "eslint-config-udes": "^0.1.6",
    "mocha": "^4.0.1",
    "nsp": "^3.1.0",
    "nyc": "^11.3.0"
  },
  "homepage": "https://github.com/UdeS-STI/udes-auth-plugins#readme"
}
